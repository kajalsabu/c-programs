#include <stdio.h>

int main()
{
    //wap to read a no.and check the following conditions
    //if it is an even no.
    //the no. contains 4 bits
    //if it has a digit 5 atleast once
    //if all the above conditions are satisfied return 1 else print 0
    int n,rem,temp,dcount=0,flageven,flag5,flag4;
     scanf("%d",&n);
    while(n>0)
    {
        rem=n%10;
        n=n/10;
        dcount++;
        if (rem=5)
        flag5=1;

    }
    if(dcount==4)
    flag4=1;
    if(temp%2==0)
    flageven=1;
    if(flag5+flag4+flageven==3)
    printf("1");
    else 
    printf("0");
}


  //years which are multiple of 100 is not a leap year but years which are a multiple of 400 is a leap year 

#include <stdio.h>

int main()
{
   int date ,i,d,m,y,count=0;
   int monthend[]={0,31,28,31,30,31,30,31,31,30,31,30,31};
   int leap=0;
   printf("enter the date:");
   scanf("%d",&date);
   y=date%10000;
   date=date/10000;
   m=date%100;
   date=date/100;
   d=date;
   if (y%4==0)
   {
       if (y%400==0)
       leap=1;
       else if (y%100==0)
       leap =0;
       else 
       leap=1;
   }
   monthend[2]=monthend[2]+leap;
   if(m<1 || m>12)
   count=-1;
   if (d<1 || d>monthend[m])
   count=-1;
   if (count!=-1 )
   {
   for (i=1;i<m;i++)
   {
       count=count +monthend[i];
   }
   count=count+d;
   }printf("%d",count);
  

   
   
   
 
    return 0;
}


//array insert
#include <stdio.h>

int main()
{
    int i, lim,arr[i],newele,pos;
    scanf("%d",&lim);
    for(i=0;i<lim;i++)
    {
        scanf("%d",&arr[i]);
    
    }
     
    //scanf("%d",&pos);
    for(i=pos-1;i<lim-1;i++)
    {
        arr[i]=arr[i+1];
    }
    lim--;
    for(i=0;i<lim-1;i++)
    {
        printf("%d",arr[i]);
    }
    
    
    for(i=lim-1;i<pos-1;i--)
    {
        arr[i+1]=arr[i];
    }
    lim++;
    for(i=0;i<lim;i++)
    {
        printf("%d",arr[i]);
    }
   return 0;


}

//array same element dlt
#include <stdio.h>

int main()
{
   //wap to delete all the occurences of a given element on a given array
   
   int arr[]={10,10,20,30,40,10,50,79};
   int i,pos,j,lim=10;
   for (i=0;i<lim;i++)
  {
   if (arr[i]==10)
   {
       pos=i;
       for(j=pos;j<lim-1;j++)
   
   {
     
   
       arr[j]=arr[j+1];
       
   }
   lim--;
   
  }
  if(arr[i]==10)
   i--;
}
   
printf("the array is:");
for(i=0;i<lim;i++)
{
    printf("\n %d ",arr[i]);
}
}

//add 1 to even and sub 1 from odd
#include <stdio.h>

int main()
{
   //wap to delete all the occurences of a given element on a given array
   
   int arr[]={10,10,20,30,40,10,50,79};
   int i,pos,j,lim=10;
   for (i=0;i<lim;i++)
  {
   if (arr[i]==10)
   {
       pos=i;
       for(j=pos;j<lim-1;j++)
   
   {
     
   
       arr[j]=arr[j+1];
       
   }
   lim--;
   
  }
  if(arr[i]==10)
   i--;
}
   
printf("the array is:");
for(i=0;i<lim;i++)
{
    printf("\n %d ",arr[i]);
}
}









    
    
